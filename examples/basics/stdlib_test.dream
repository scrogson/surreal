pub fn main() -> atom {
    let items = list::range(1, 5);
    let reversed = list::reverse(items);

    io::format("Items: ~p~n", [items]);
    io::format("Reversed list: ~p~n", [reversed]);

    io::println("Hello from dream!");

    :io::format("Sum of 10 and 20 is ~p~n", [10 + 20]);

    :ok
